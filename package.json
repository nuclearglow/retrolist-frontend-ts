{
  "private": true,
  "name": "retrolist-frontend",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "start": "snowpack dev",
    "build": "snowpack build",
    "build:serve": "sirv build --port 8080 --cors --single",
    "deploy": "node ./scripts/deploy.js",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:stylelint": "stylelint 'src/*.{css,tsx}'",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.tsx": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-div-100vh": "^0.6.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@apollo/link-error": "^2.0.0-beta.3",
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/time": "^2.0.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.6",
    "@typed/content-hash": "^0.10.0",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/snowpack-env": "^2.3.3",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@web/test-runner": "^0.13.3",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "du": "^1.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "node-ssh": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "sirv-cli": "^1.0.12",
    "snowpack": "^3.3.7",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^4.2.4"
  }
}
